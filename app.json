{
  "expo": {
    "name": "Val-entine",
    "slug": "Val-entine",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/valIcon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/valIcon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ccraig09.Valentine"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
